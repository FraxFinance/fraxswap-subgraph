specVersion: 0.0.4
schema:
  file: ./schema.graphql
dataSources:
  - kind: ethereum
    name: FraxswapFactory
    network: mainnet
    source:
      address: "0xB076b06F669e682609fb4a8C6646D2619717Be4b"
      abi: FraxswapFactory
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.5
      language: wasm/assemblyscript
      entities:
        - PairCreated
      abis:
        - name: FraxswapFactory
          file: ./abis/FraxswapFactory.json
      eventHandlers:
        - event: PairCreated(indexed address,indexed address,address,uint256)
          handler: handlePairCreated
      file: ./src/fraxswap-factory.ts
  - kind: ethereum
    name: FraxswapPair
    network: mainnet
    source:
      address: "0x8206412c107eF1aDb70B9277974f5163760E128E"
      abi: FraxswapPair
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.5
      language: wasm/assemblyscript
      entities:
        - Approval
        - Burn
        - CancelLongTermOrder
        - LongTermSwap0To1
        - LongTermSwap1To0
        - Mint
        - Swap
        - Sync
        - Transfer
        - VirtualOrderExecution
        - WithdrawProceedsFromLongTermOrder
      abis:
        - name: FraxswapPair
          file: ./abis/FraxswapPair.json
      eventHandlers:
        - event: Approval(indexed address,indexed address,uint256)
          handler: handleApproval
        - event: Burn(indexed address,uint256,uint256,indexed address)
          handler: handleBurn
        - event: CancelLongTermOrder(indexed address,uint256,address,uint256,address,uint256)
          handler: handleCancelLongTermOrder
        - event: LongTermSwap0To1(indexed address,uint256,uint256,uint256)
          handler: handleLongTermSwap0To1
        - event: LongTermSwap1To0(indexed address,uint256,uint256,uint256)
          handler: handleLongTermSwap1To0
        - event: Mint(indexed address,uint256,uint256)
          handler: handleMint
        - event: Swap(indexed address,uint256,uint256,uint256,uint256,indexed address)
          handler: handleSwap
        - event: Sync(uint112,uint112)
          handler: handleSync
        - event: Transfer(indexed address,indexed address,uint256)
          handler: handleTransfer
        - event: VirtualOrderExecution(uint256,uint256,uint256,uint256,uint256,uint256,uint256,uint256,uint256,uint256)
          handler: handleVirtualOrderExecution
        - event: WithdrawProceedsFromLongTermOrder(indexed address,uint256,indexed
            address,uint256,bool)
          handler: handleWithdrawProceedsFromLongTermOrder
      file: ./src/fraxswap-pair.ts
